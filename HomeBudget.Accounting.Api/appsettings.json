{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "AWSSDK": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "WithThreadId", "WithMachineName" ]
  },
  "HealthCheckOptions": {
    "Host": "http://homebudget-accounting-api"
  },
  "EventStoreDb": {
    "Url": ""
  },
  "ElasticConfiguration": {
    "Uri": ""
    "Uri": "",
  },
  "MongoDbOptions": {
    "ConnectionString": "",
    "PaymentAccountsDatabaseName": "payment-accounts",
    "PaymentsHistoryDatabaseName": "payments-history",
    "HandBooksDatabaseName": "handbooks"
  },
  "KafkaOptions": {
    "ProducerSettings": {
      "BootstrapServers": ""
    },
    "Topics": {
      "RequestLoggerTopic": "topic-name"
    }
  },
  "UiOriginsUrl": [ "*" ]
}
