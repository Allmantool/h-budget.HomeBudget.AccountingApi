      "Microsoft.AspNetCore": "Warning"
    }
  },
  "EventStoreDb": {
    "Url": "esdb://event-store-db:2113/?tls=false&keepAliveTimeout=10000&keepAliveInterval=10000"
  },
  "ElasticConfiguration": {
    "Uri": "http://elasticsearch:9200"
  },
