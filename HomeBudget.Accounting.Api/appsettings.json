{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "AWSSDK": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "WithThreadId", "WithMachineName" ]
  },
  "HealthCheckOptions": {
    "Host": ""
  },
  "ObservabilityOptions": {
    "TelemetryEndpoint": "",
    "LogsEndpoint": ""
  },
  "EventStoreDb": {
    "Url": "",
    "TimeoutInSeconds": 90,
    "RetryAttempts": 5
  },
  "ElasticSearchOptions": {
    "IsEnabled": false,
    "Uri": ""
  },
  "SeqOptions": {
    "IsEnabled": true,
    "Uri": ""
  },
  "MongoDbOptions": {
    "ConnectionString": "",
    "PaymentAccounts": "payment_accounts",
    "PaymentsHistory": "payments_history",
    "HandBooks": "handbooks",
    "LedgerDatabase": "ledger"
  },
  "Sentry": {
    "Dsn": ""
  },
  "KafkaOptions": {
    "ProducerSettings": {
      "BootstrapServers": ""
    },
    "ConsumerSettings": {
      "BootstrapServers": ""
    },
    "AdminSettings": {
      "BootstrapServers": ""
    },
    "Topics": {
      "RequestLoggerTopic": "topic-name"
    }
  },
  "UiOriginsUrl": [ "*" ]
}
